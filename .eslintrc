{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "airbnb-base"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [

    ],
    "rules": {
      "eslint-disable": "no-unused-vars",
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "import/no-extraneous-dependencies": ["error", {"devDependencies": false, "optionalDependencies": false, "peerDependencies": false}],
      "linebreak-style": 0
    }
}
